<odoo>
    <data>
        <report id="dimabe_quotation_custom"
            string="Presupuesto Dimabe"
            model="sale.order"
            report_type="qweb-pdf"
            name="dimabe_quotation_report.dimabe_quotation_custom_report"
        />
        <template id="dimabe_quotation_custom_report" inherit_id="web.external_layout_clean">
            <xpath expr="//div[@class='header']" position="replace">
                <div class="header" style="font-family: 'Roboto', sans-serif;
                                        font-weight: bold;">
                    <h1>
                        Header Modificado
                    </h1>
                </div>
            </xpath>
        </template>
    </data>
</odoo>